<p1:Tab  xmlns="http://www.eclipse.org/xwt/presentation" xmlns:x="http://www.eclipse.org/xwt" xmlns:p1="clr-namespace:com.ifx.davex.ui.controls.app" x:Class="e">
	<p1:TabFolder bounds="1, 3, 557, 953">
		<p1:GTab text="General" visible="true" enabled="true" manifestObj="true" widgetName="gtab_general">
			<p1:GTab.control>
				<p1:Tab widgetName="composite_102">
					<p1:GGroup text="Operation Mode" manifestObj="true" widgetName="ggroup_operation_mode" bounds="16, 9, 517, 97">
						<p1:GLabel bounds="12, 23, 73, 15" manifestObj="true" text="Device type:" widgetName="glabel_device_type"/>
						<p1:GCombo bounds="327, 20, 174, 21" description="Device or bus mode kind.&lt;br&gt;&#13;&#10;&lt;ul&gt;&#13;&#10;&lt;li&gt;Master: the peripheral operates as the master in a SPI communication, that is, it is the one that provides the clock.&lt;/li&gt;&#13;&#10;&lt;li&gt;Slave: the peripheral operates as the slave in a SPI communication. It needs the clock provided by a master for communication.&lt;/li&gt;&#13;&#10;&lt;/ul&gt;" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="Master&#13;&#10;Slave" value="Master" widgetName="gcombo_device_type" toolTipText="Configures USIC peripheral channel to operate as an&#13;&#10;either master or slave device type."/>
						<p1:GLabel bounds="12, 58, 115, 15" manifestObj="true" text="Operation mode:" widgetName="glabel_operation_mode"/>
						<p1:GCombo bounds="327, 55, 174, 21" description="Desired operation mode for the data transmission and reception.&lt;br&gt;&#13;&#10;&lt;ul&gt;&#13;&#10;&lt;li&gt;Half Duplex: One common pin is used for transmission and reception.&#13;&#10;In this mode, connection can be established with external or internal techniques based on signal connections with DIGITAL_IO APP.&lt;br&gt;&#13;&#10;In external half duplex connection technique: Two instance of DIGITAL_IO APP is used for MOSI and MISO pins, i.e. one DIGITAL_IO APP for MOSI signal and other one is for MISO signal connection. User need to ensure these signal connection manually between DIGITAL_IO and SPI_INIT APPs. And also need to ensure external short between MOSI and MISO pins.&lt;br&gt;&#13;&#10;In internal half duplex connection technique: Only one instance of DIGITAL_IO APP is used for MOSI and MISO signal connections. This DIGITAL_IO APP pin act as a MOSI/MISO line i.e. single communication line is shared for transmission and reception. User need to ensure these signal connection manually between DIGITAL_IO and SPI_INIT APPs.&lt;/li&gt;&#13;&#10;&lt;li&gt;Full Duplex: Two separate pins are used for transmission and reception.&lt;/li&gt;&#13;&#10;&lt;li&gt;Dual SPI: Two pins are used for both transmission and reception. Switching between input and output mode is done automatically by the hardware.&lt;/li&gt;&#13;&#10;&lt;li&gt;Quad SPI: Four pins are used for both transmission and reception. Switching between input and output mode is done automatically by the hardware.&lt;/li&gt;&#13;&#10;&lt;li&gt;Loop Back: No pins are used. When data is transmitted, the same data will be received.&lt;/li&gt;&#13;&#10;&lt;/ul&gt;" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="Half Duplex&#13;&#10;Full Duplex&#13;&#10;Dual SPI&#13;&#10;Quad SPI&#13;&#10;Loop Back" value="Full Duplex" widgetName="gcombo_operation_mode" toolTipText="Configures operation mode for the data transmission and reception."/>
					</p1:GGroup>
					<p1:GGroup bounds="16, 410, 517, 297" manifestObj="true" text="Input Stages" widgetName="ggroup_input_stages">
						<p1:GLabel bounds="12, 23, 115, 15" manifestObj="true" text="DX1 input stage:" widgetName="glabel_dx1_input_stage"/>
						<p1:GLabel bounds="12, 84, 115, 15" manifestObj="true" text="DX2 input stage:" widgetName="glabel_dx2_input_stage"/>
						<p1:GCombo bounds="327, 81, 174, 21" description="Input signal chosen in slave mode for the input stage DX2. This setting can&apos;t be changed, as the mandatory SELIN signal is the input." mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="SELIN" value="SELIN" widgetName="gcombo_dx2_input_stage_slave" toolTipText="Configures input signal source for the input stage DX2."/>
						<p1:GCombo bounds="327, 20, 174, 21" description="Input signal chosen in slave mode for the input stage DX1. This setting can&apos;t be changed, as the mandatory SCLKIN signal is the input." mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="SCLKIN" value="SCLKIN" widgetName="gcombo_dx1_input_stage_slave" toolTipText="Configures input signal source for the input stage DX1."/>
						<p1:GCombo bounds="327, 20, 174, 21" description="Input signal chosen in master mode for the input stage DX1.&lt;br&gt;&#13;&#10;&lt;ul&gt;&#13;&#10;&lt;li&gt;None: No signal is used&lt;/li&gt;&#13;&#10;&lt;li&gt;External Frequency Input: used for the baud rate generation. This allows transfers with a frequency that can not be generated internally.&lt;/li&gt;&#13;&#10;&lt;li&gt;Delay Compensation: The master receives the shift clock back from the slave&lt;/li&gt;&#13;&#10;&lt;/ul&gt;" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="None&#13;&#10;External Frequency Input&#13;&#10;Delay Compensation" value="None" widgetName="gcombo_dx1_input_stage_master" toolTipText="Configures input signal source for the input stage DX1."/>
						<p1:GCombo bounds="327, 81, 174, 21" description="Input signal chosen for the input stage DX2.&lt;br&gt;&#13;&#10;&lt;ul&gt;&#13;&#10;&lt;li&gt;None: No signal is used&lt;/li&gt;&#13;&#10;&lt;li&gt;Transmit Data Validation: A singal is used for transmit data validation purposes&lt;/li&gt;&#13;&#10;&lt;li&gt;Delay Compensation: The master receives the slave select signal back from the slave&lt;/li&gt;&#13;&#10;&lt;/ul&gt;" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="None&#13;&#10;Transmit Data Validation&#13;&#10;Delay Compensation" value="None" widgetName="gcombo_dx2_input_stage_master" toolTipText="Configures input signal source for the input stage DX2."/>
						<p1:GCheck bounds="12, 53, 246, 16" description="Selection inverts the signal polarity of the input signal DX1." manifestObj="true" text="Invert signal polarity of DX1" widgetName="gcheck_enable_invert_dx1_input_stage" toolTipText="Bit field (DX1CR.DPOL)&#13;&#10;Selection inverts the signal polarity of the input signal DX1."/>
						<p1:GCheck bounds="12, 114, 246, 16" description="Selection inverts the signal polarity of the input signal DX2." manifestObj="true" text="Invert signal polarity of DX2" widgetName="gcheck_enable_invert_dx2_input_stage" toolTipText="Bit field (DX2CR.DPOL)&#13;&#10;Selection inverts the signal polarity of the input signal DX2."/>
						<p1:GMessage manifestObj="true" text="For the delay compensation in master mode to work, the &quot;Delay Compensation&quot; input has to be selected in the DX1 Input Stage." widgetName="gmessage_delay_compensation_method_master" bounds="13, 205, 485, 39"/>
						<p1:GMessage manifestObj="true" text="Using the &quot;No delay, polarity inversion&quot; setting for the Shift Clock, the polarity inversion needs to be inverted for the DX1 input stage." widgetName="gmessage_invert_dx1_input_stage" bounds="13, 249, 485, 39"/>
						<p1:GLabel bounds="12, 145, 273, 15" manifestObj="true" text="Type of data validation:" widgetName="glabel_type_transmit_data_validation"/>
						<p1:GCombo bounds="327, 141, 174, 21" description="Type of transmit data validation used.&lt;br&gt;&#13;&#10;&lt;ul&gt;&#13;&#10;&lt;li&gt;Transfer on low (DX2S = 0): Transfer gating logic is used. A transmission of the data word in TBUF can be started if the value of the signal DX2S is equal to zero.&lt;/li&gt;&#13;&#10;&lt;li&gt;Transfer on high (DX2S = 1):  Also the transfer gating logic is used, but in this case, the transmission can be started if the value of the signal DX2S is equal to one.&lt;/li&gt;&#13;&#10;&lt;li&gt;Transfer on event (DX2T): A transfer trigger logic is made use of. Data words transfers are triggered by events.&lt;/li&gt;&#13;&#10;&lt;/ul&gt;" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="Transfer On Low (DX2S = 0)&#13;&#10;Transfer On High (DX2S = 1)&#13;&#10;Transfer On Event (DX2T)" value="Transfer On Low (DX2S = 0)" widgetName="gcombo_type_transmit_data_validation" toolTipText="Bit field (TCSR.TDEN / TCSR.TDVTR)&#13;&#10;Selection controls the gating of the transmission&#13;&#10;start of the data word in the transmit buffer TBUF."/>
						<p1:GLabel bounds="12, 175, 307, 15" manifestObj="true" text="Triggering edge for data validation on event:" widgetName="glabel_transmit_data_validation_trigger_edge"/>
						<p1:GCombo bounds="327, 170, 174, 21" description="This field defines the edge of the DX2T signal to be used for triggering the transmit data validation.&lt;br&gt;&#13;&#10;&lt;ul&gt;&#13;&#10;&lt;li&gt;Rising Edge: The rising edge of the DX2T signal is used.&lt;/li&gt;&#13;&#10;&lt;li&gt;Falling Edge: The rising edge of the DX2T signal is used.&lt;/li&gt;&#13;&#10;&lt;li&gt;Both Edges: Both edges of the DX2T signal is used.&lt;/li&gt;&#13;&#10;&lt;/ul&gt;" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="Rising Edge&#13;&#10;Falling Edge&#13;&#10;Both Edges" value="Rising Edge" widgetName="gcombo_transmit_data_validation_trigger_edge" toolTipText="Bit field (DX2CR.CM)&#13;&#10;Selection activates the DX2S signal edge for trigger output DXnT."/>
					</p1:GGroup>
					<p1:GGroup bounds="16, 723, 517, 124" manifestObj="true" text="Output Signals" widgetName="ggroup_outputs_signals">
						<p1:GLabel bounds="12, 28, 115, 15" manifestObj="true" text="SCLKOUT output:" widgetName="glabel_sclkout_output"/>
						<p1:GLabel bounds="12, 60, 115, 15" manifestObj="true" text="MCLKOUT output:" widgetName="glabel_mclkout_output"/>
						<p1:GCombo bounds="327, 57, 174, 21" description="The MCLKOUT output signal has a stable&#13;&#10;frequency relation to the shift clock output &#13;&#10;(the frequency of MCLKOUT can be higher&#13;&#10;than for SCLKOUT) for synchronization purposes&#13;&#10;of a slave device to a master device." mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="None&#13;&#10;Independent Clock Output" value="None" widgetName="gcombo_mclkout_output_slave" toolTipText="The MCLKOUT output signal has a stable&#13;&#10;frequency relation to the shift clock output &#13;&#10;(the frequency of MCLKOUT can be higher&#13;&#10;than for SCLKOUT) for synchronization purposes&#13;&#10;of a slave device to a master device."/>
						<p1:GCombo bounds="327, 25, 174, 21" description="In slave mode, the shift clock is generated by master and &#13;&#10;the internal baud rate generator of the slave is not needed. &#13;&#10;The SCLKOUT and MCLKOUT signals can be used as clock &#13;&#10;outputs with 50% duty cycle with a frequency that can be&#13;&#10;independent from the communication baud rate." mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="None&#13;&#10;Independent Clock Output" value="None" widgetName="gcombo_sclkout_output_slave" toolTipText="In slave mode, the shift clock is generated by master and &#13;&#10;the internal baud rate generator of the slave is not needed. &#13;&#10;The SCLKOUT and MCLKOUT signals can be used as clock &#13;&#10;outputs with 50% duty cycle with a frequency that can be&#13;&#10;independent from the communication baud rate."/>
						<p1:GCombo bounds="327, 25, 174, 21" description="In master mode, the shift clock is generated by&#13;&#10;the internal baud rate generator. The output signal SCLKOUT&#13;&#10;of the baud rate generator is taken as shift clock input for the &#13;&#10;data shift unit." mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="Master Shift Clock" value="Master Shift Clock" widgetName="gcombo_sclkout_output_master" toolTipText="In master mode, the shift clock is generated by&#13;&#10;the internal baud rate generator. The output signal SCLKOUT&#13;&#10;of the baud rate generator is taken as shift clock input for the &#13;&#10;data shift unit."/>
						<p1:GCombo bounds="327, 57, 174, 21" description="The MCLKOUT output signal has a stable&#13;&#10;frequency relation to the shift clock output &#13;&#10;(the frequency of MCLKOUT can be higher&#13;&#10;than for SCLKOUT) for synchronization purposes&#13;&#10;of a slave device to a master device." mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="None&#13;&#10;Master Time Base" value="None" widgetName="gcombo_mclkout_output_master" toolTipText="The MCLKOUT output signal has a stable&#13;&#10;frequency relation to the shift clock output &#13;&#10;(the frequency of MCLKOUT can be higher&#13;&#10;than for SCLKOUT) for synchronization purposes&#13;&#10;of a slave device to a master device."/>
						<p1:GCheck bounds="12, 92, 306, 16" description="Selection inverts the signal polarity of the data output signal DOUTx." manifestObj="true" text="Invert signal polarity of output signals" widgetName="gcheck_enable_invert_outputs" toolTipText="Bit field (SCTR.DOCFG)&#13;&#10;Selection inverts the signal polarity of the data output signal DOUTx."/>
					</p1:GGroup>
					<p1:GGroup bounds="16, 122, 517, 231" manifestObj="true" text="Frame Format" widgetName="ggroup_frame_format">
						<p1:GCombo bounds="327, 130, 174, 21" description="Parity mode selection for the communication. Ensure that the communication counterpart also uses the same parity mode.&lt;br&gt;&#13;&#10;&lt;ul&gt;&#13;&#10;&lt;li&gt;No Parity: No extra bit to indicate the data parity.&lt;/li&gt;&#13;&#10;&lt;li&gt;Even Parity: One extra bit will be added after the last data bit. This bit will be logic 1 if there are even number of logic 1 bits in the data.&lt;/li&gt;&#13;&#10;&lt;li&gt;Odd Parity: One extra bit will be added after the last data bit. This bit will be logic 1 if there are odd number of logic 1 bits in the data.&lt;/li&gt;&#13;&#10;&lt;/ul&gt;" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="No Parity&#13;&#10;Even Parity&#13;&#10;Odd Parity" value="No Parity" widgetName="gcombo_parity" toolTipText="Bit field (CCR.PM)&#13;&#10;Configures parity mode for detecting the error&#13;&#10;during communication."/>
						<p1:GLabel bounds="12, 129, 140, 16" manifestObj="true" text="Type of parity:" widgetName="glabel_parity"/>
						<p1:GLabel bounds="12, 24, 170, 16" manifestObj="true" text="Word length:" widgetName="glabel_word_length" toolTipText=""/>
						<p1:GLabel bounds="12, 94, 173, 16" manifestObj="true" text="Data shift direction:" widgetName="glabel_data_shift_dir"/>
						<p1:GCombo bounds="327, 92, 174, 21" description="Shift direction of the data words for transmission and reception.&lt;br&gt;&#13;&#10;&lt;ul&gt;&#13;&#10;&lt;li&gt;Shift LSB (Least Significant Bit) first: The first data bit of a data word is located at bit position 0.&lt;/li&gt;&#13;&#10;&lt;li&gt;Shift MSB (Most Significant Bit) first: The first data bit of a data word is located at the bit position given by bit field SCTR.WLE&lt;/li&gt;&#13;&#10;&lt;/ul&gt;&#13;&#10;&lt;br&gt;" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="LSB&#13;&#10;MSB" value="MSB" widgetName="gcombo_data_shift_dir" toolTipText="Bit field (SCTR.SDIR)&#13;&#10;Configures shift direction of the data words for transmission and reception."/>
						<p1:GInteger bounds="327, 21, 174, 23" description="This defines the data word length (amount of&#13;&#10;bits that are transferred in each data word) for&#13;&#10;reception and transmission." format="(com.ifx.davex.ui.controls.util.AppUiConstants).DEC" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" maxValue="16" minValue="1" value="8" widgetName="ginteger_word_length" toolTipText="Bit field (SCTR.WLE + 1)&#13;&#10;Configures number of bits required in each data word."/>
						<p1:GLabel bounds="12, 59, 170, 16" manifestObj="true" text="Frame length:" widgetName="glabel_frame_length" toolTipText=""/>
						<p1:GInteger bounds="327, 56, 174, 23" description="This defines how many bits are transferred within a data frame." format="(com.ifx.davex.ui.controls.util.AppUiConstants).DEC" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" maxValue="64" minValue="1" value="64" widgetName="ginteger_frame_length" toolTipText="Bit field (SCTR.FLE + 1)&#13;&#10;Configures number of bits required in per data frame."/>
						<p1:GCheck text="Keep the Slave Select signal active after a frame has been transmitted" manifestObj="true" widgetName="gcheck_enable_frame_end_mode" bounds="12, 164, 450, 16" description="Enable the frame end mode." toolTipText="Bit field (PCR.FEM)&#13;&#10;Configures frame end mode. &#13;&#10;Selection allows MSLS signal to be active also while no new&#13;&#10;data is available and no other end of frame condition is reached."/>
						<p1:GMessage bounds="13, 187, 485, 37" manifestObj="true" text="If the frame length is 64 data bits or greater, parity bit generation is not supported." widgetName="gmessage_frame_length"/>
					</p1:GGroup>
					<p1:GCheck bounds="16, 375, 166, 16" description="Selections enables delay compensation method for the master." manifestObj="true" text="Enable delay compensation" toolTipText="Bit field (DX1CR.DCEN)&#13;&#10;Selections enables delay compensation method for the master." widgetName="gcheck_enable_delay_compensation_master"/>
					<p1:GCheck bounds="16, 376, 166, 16" description="Enables delay compensation." manifestObj="true" text="Enable delay compensation" toolTipText="Selection enables delay compensation method for the slave.&#13;&#10;Only complete closed-loop delay compensation can be configured." widgetName="gcheck_enable_delay_compensation_slave"/>
				</p1:Tab>
			</p1:GTab.control>
		</p1:GTab>
		<p1:GTab text="Shift Clock" visible="true" enabled="true" manifestObj="true" widgetName="gtab_shift_clock">
			<p1:GTab.control>
				<p1:Tab widgetName="composite_42">
					<p1:GGroup bounds="16, 79, 517, 394" manifestObj="true" text="Baud Rate Generator" widgetName="ggroup_baud_rate">
						<p1:GLabel bounds="13, 61, 171, 15" manifestObj="true" text="Desired bus speed [kHz]:" widgetName="glabel_desired_baud_rate"/>
						<p1:GLabel bounds="13, 96, 171, 15" manifestObj="true" text="Divider factor:" widgetName="glabel_divider_factor"/>
						<p1:GInteger bounds="350, 92, 148, 21" description="This field indicates the ratio between the input frequency fPPP and the divider frequency fPDIV." mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" maxValue="1024" minValue="1" value="1" widgetName="ginteger_divider_factor_internal_clk" enabled="false" toolTipText="Bit field (BRG.PDIV + 1)&#13;&#10;Configures ratio between the input frequency fPPP&#13;&#10;and the divider frequency fPDIV."/>
						<p1:GLabel bounds="13, 334, 171, 15" manifestObj="true" text="Actual bus speed [kHz]:" widgetName="glabel_actual_baud_rate"/>
						<p1:GFloat bounds="350, 331, 148, 21" description="Indicates the actual baud that can be achieved with the device. This value is calculated based on the entered value of desired baud rate." enabled="false" format="*,2" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" maxValue="POSITIVE_INFINITY" minValue="NEGATIVE_INFINITY" value="19.2" widgetName="gfloat_actual_baud_rate" x:Style="BORDER" toolTipText="Indicates the actual baud that can be achieved with the device.&#13;&#10;This value is calculated based on the entered value of desired baud rate."/>
						<p1:GFloat bounds="350, 58, 148, 21" description="Allows to configure the desired baud rate for the SPI communication. The value should be specified in baud. The maximum value of the baud rate should be calculated based on peripheral clock and the oversampling settings. Max baud = (fPERIPH * 1023)/(1024 * oversampling)." format="*,2" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" maxValue="60000" minValue="0.1" value="19.2" widgetName="gfloat_desired_baud_rate" x:Style="BORDER" toolTipText="Configure the desired baud rate for the SPI communication."/>
						<p1:GLabel bounds="13, 26, 110, 15" manifestObj="true" text="Clock:" widgetName="glabel_clock_selection"/>
						<p1:GCombo bounds="350, 23, 148, 21" description="This field defines the frequency fPIN, input to the dividers of the BRG.&lt;br&gt;&#13;&#10;&lt;ul&gt;&#13;&#10;&lt;li&gt;Internal Source (FD): The fractional divider frequency fFD is selected.&lt;/li&gt;&#13;&#10;&lt;li&gt;External Source (DX1T): The trigger signal DX1T defines fPIN.&lt;/li&gt;&#13;&#10;&lt;/ul&gt;" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="Internal Source (FD)&#13;&#10;External Source (DX1T)" value="Internal Source (FD)" widgetName="gcombo_clock_selection" toolTipText="Bit field (BRG.CLKSEL)&#13;&#10;Configures baud rate generator input frequency (fPIN) source."/>
						<p1:GLabel bounds="13, 61, 154, 15" manifestObj="true" text="Edge selection:" widgetName="glabel_external_clock_edge"/>
						<p1:GCombo bounds="350, 58, 148, 21" description="This field selects which edge of the external source signal (DX1S) must be used to generate the clock signal (DX1T)&#13;&#10;&lt;ul&gt;&#13;&#10;&lt;li&gt;Rising Edge: The rising edge of the external signal generates the clock signal.&lt;/li&gt;&#13;&#10;&lt;li&gt;Falling Edge: The falling edge of the external signal generates the clock signal.&lt;/li&gt;&#13;&#10;&lt;li&gt;Both Edges: Both edges of the external signal generate the clock signal.&lt;/li&gt;&#13;&#10;&lt;/ul&gt;" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="Rising Edge&#13;&#10;Falling Edge&#13;&#10;Both Edges" value="Rising Edge" widgetName="gcombo_external_clock_edge" toolTipText="Bit field (DX1CR.CM)&#13;&#10;Selection activates the DX1S signal edge for trigger output DXnT."/>
						<p1:GImage visible="true" manifestObj="true" widgetName="gimage_baud_rate" bounds="38, 127, 440, 195" path="doc/SPI_CONFIG_BRG_internal_clk.jpg" x:Style="BORDER"/>
						<p1:GInteger bounds="350, 92, 148, 21" description="This field indicates the ratio between the input frequency fPPP and the divider frequency fPDIV." mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" maxValue="1024" minValue="1" value="1" widgetName="ginteger_divider_factor_external_clk" toolTipText="Bit field (BRG.PDIV + 1)&#13;&#10;Configures ratio between the input frequency fPPP and the divider frequency fPDIV."/>
						<p1:GCheck bounds="13, 364, 286, 16" description="On selecting this option, the capture mode timer is enabled." manifestObj="true" text="Enable the capture mode timer" widgetName="gcheck_enable_capture_mode_timer" toolTipText="Bit field (BRG.TMEN)&#13;&#10;On selecting this option, the capture mode timer is enabled."/>
					</p1:GGroup>
					<p1:GLabel bounds="18, 9, 478, 31" manifestObj="true" text="Shift clock signal (SCLKOUT) setting with respect to the shift clock (SCLK) signal from the&#13;&#10;Baud Rate Generator: " widgetName="glabel_shift_clock_setting"/>
					<p1:GCombo bounds="18, 44, 316, 21" description="Configures the required delay and signal polarity level for the shift clock output signal SCLKOUT with respect to SCLK.&lt;br&gt;&#13;&#10;&lt;ul&gt;&#13;&#10;&lt;li&gt;No Delay, No Polarity Inversion: SCLKCFG = 00B, SCLKOUT equals SCLK.&lt;/li&gt;&#13;&#10;&lt;li&gt;No Delay, Polarity Inversion:  SCLKCFG = 01B, SCLKOUT inverted to SCLK.&lt;/li&gt;&#13;&#10;&lt;li&gt;Delayed By 1/2 Shift Clock Period, No Polarity Inversion: SCLKCFG = 10B, SCLKOUT delayed / shifted by 1/2 SCLK Clock Period. &lt;/li&gt;&#13;&#10;&lt;li&gt;Delayed By 1/2 Shift Clock Period, Polarity Inversion: SCLKCFG = 11B, SCLKOUT delayed / shifted by 1/2 SCLK Clock Period and inverted to SCLK. &lt;/li&gt;&#13;&#10;&lt;/ul&gt;" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="No Delay, No Polarity Inversion&#13;&#10;No Delay, Polarity Inversion&#13;&#10;Delayed By 1/2 Shift Clock Period, No Polarity Inversion&#13;&#10;Delayed By 1/2 Shift Clock Period, Polarity Inversion" value="No Delay, No Polarity Inversion" widgetName="gcombo_shift_clock_setting" toolTipText="Bit field (BRG.SCLKCFG )&#13;&#10;Configures the required delay and signal polarity level&#13;&#10;for the shift clock output signal SCLKOUT with respect to SCLK."/>
				</p1:Tab>
			</p1:GTab.control>
		</p1:GTab>
		<p1:GTab text="Slave Select" visible="true" enabled="true" manifestObj="true" widgetName="gtab_slave_select">
			<p1:GTab.control>
				<p1:Tab widgetName="composite_50" location="-36, 24">
					<p1:GGroup bounds="17, 144, 517, 185" manifestObj="true" text="Leading/Trailing Delay Generation" widgetName="ggroup_slave_select_ld_td_generation">
						<p1:GLabel bounds="12, 22, 171, 15" manifestObj="true" text="Desired delay [clock cycles]:" widgetName="glabel_desired_ld_td_value"/>
						<p1:GInteger bounds="424, 18, 74, 23" description="Configures slave setup and hold time requirements. " mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" maxValue="32" minValue="1" value="2" widgetName="ginteger_desired_ld_td_value" toolTipText="Configures slave setup and hold time requirements. "/>
						<p1:GLabel bounds="12, 150, 171, 15" manifestObj="true" text="Actual delay [us]:" widgetName="glabel_actual_ld_td_value"/>
